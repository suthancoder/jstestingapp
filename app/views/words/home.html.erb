 <div class="container">

  <h1>Learning tool menu!</h1>

</div>


<div id="restaurant_data" class="container" data-reviews="<%= @reviews  %>">


<% @restaurants.each do |restaurant| %>
  <div class="row"   >
    <div class="card col-xs-3" id="reviews">
      <p>Good place</p>
    </div>
    <div class="card col-xs-3" id="restaurant">
      <p><%= link_to restaurant, review_path, remote: true %></p>
    </div>
    <div class="card col-xs-3">
      <p>A Mills</p>
    </div>
    <div class="card col-xs-3">
      <p>25 Smith Road</p>
      <%= render "show", review2: "my html review" %>
    </div>
  </div>
  <% end %>


</div>




<div class="container">

  <h2>
    Back to all the tests

  </h2>
  <%= link_to "Home", root_path, :class => "btn btn-primary" %>
</div>
